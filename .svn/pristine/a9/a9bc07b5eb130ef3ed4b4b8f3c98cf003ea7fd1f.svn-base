<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_sheet_sDef</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<idsite>
						<TCEforms>
							<label>LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_idsite_selection</label>
							<config>
								<type>select</type>
								<items type="array">
								</items>
								<itemsProcFunc>EXT:piwikintegration/pi1/class.tx_piwikintegration_flexform.php:tx_piwikintegration_flexform->getSitesForFlexForm</itemsProcFunc>
								<iconsInOptionTags>1</iconsInOptionTags>
								<noIconsBelowSelect>1</noIconsBelowSelect>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</idsite>
					<widget>
						<TCEforms>
							<label>LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_widget_selection</label>
							<config>
								<type>select</type>
								<items type="array">
								</items>
								<itemsProcFunc>EXT:piwikintegration/pi1/class.tx_piwikintegration_flexform.php:tx_piwikintegration_flexform->getWidgetsForFlexForm</itemsProcFunc>
								<iconsInOptionTags>1</iconsInOptionTags>
								<noIconsBelowSelect>1</noIconsBelowSelect>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</widget>
					<period>
						<TCEforms>
							<label>LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_period</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_period_day</numIndex>
										<numIndex index="1">day</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_period_week</numIndex>
										<numIndex index="1">week</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_period_month</numIndex>
										<numIndex index="1">month</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_period_year</numIndex>
										<numIndex index="1">year</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_period_yesterday</numIndex>
										<numIndex index="1">yesterday</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</period>
					<viewDataTable>
						<TCEforms>
							<label>LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_default</numIndex>
										<numIndex index="1"></numIndex>
										<numIndex index="2">typo3.png</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_tableAllColumns</numIndex>
										<numIndex index="1">tableAllColumns</numIndex>
										<numIndex index="2">../typo3conf/piwik/piwik/themes/default/images/table.png</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_tableGoals</numIndex>
										<numIndex index="1">tableGoals</numIndex>
										<numIndex index="2">../typo3conf/piwik/piwik/themes/default/images/goal.png</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_table</numIndex>
										<numIndex index="1">table</numIndex>
										<numIndex index="2">../typo3conf/piwik/piwik/themes/default/images/table.png</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_cloud</numIndex>
										<numIndex index="1">cloud</numIndex>
										<numIndex index="2">../typo3conf/piwik/piwik/themes/default/images/tagcloud.png</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_graphPie</numIndex>
										<numIndex index="1">graphPie</numIndex>
										<numIndex index="2">../typo3conf/piwik/piwik/themes/default/images/chart_pie.png</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_graphVerticalBar</numIndex>
										<numIndex index="1">graphVerticalBar</numIndex>
										<numIndex index="2">../typo3conf/piwik/piwik/themes/default/images/chart_bar.png</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_viewDataTable_graphEvolution</numIndex>
										<numIndex index="1">graphEvolution</numIndex>
									</numIndex>
								</items>
								<iconsInOptionTags>1</iconsInOptionTags>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</viewDataTable>
					<div_height>
						<TCEforms>
							<label>LLL:EXT:piwikintegration/pi1/locallang.xml:flexform_div_height</label>
							<config>
								<type>input</type>
								<size>4</size>
							</config>
						</TCEforms>
					</div_height>
				</el>
			</ROOT>
		</sDEF>
    </sheets>
</T3DataStructure>